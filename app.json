{
  "expo": {
    "name": "IALFM",
    "slug": "ialfm-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ialfm",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E8B57"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "maariz.ialfm.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#2E8B57"
      },
      "edgeToEdgeEnabled": true,
      "package": "maariz.ialfm.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "description": "Official mobile app for the Islamic Association of Lewisville and Flower Mound (IALFM). Access prayer times, events, programs, and stay connected with our community.",
    "keywords": [
      "islam",
      "mosque",
      "masjid",
      "prayer times",
      "community",
      "lewisville",
      "flower mound",
      "ialfm"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ae0acd0a-7a55-4381-9d63-966566171c3b"
      }
    }
  }
}
